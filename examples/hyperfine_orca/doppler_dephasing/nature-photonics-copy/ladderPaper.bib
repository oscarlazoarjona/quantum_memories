issn = {2469-9926},
journal = {Physical Review A},
mendeley-groups = {Rubidium/LadderMemory,LadderPaper},
number = {5},
pages = {5},
title = {{Rydberg polaritons in a thermal vapor}},
url = {http://arxiv.org/abs/1603.05924},
volume = {93},
year = {2016}
}



