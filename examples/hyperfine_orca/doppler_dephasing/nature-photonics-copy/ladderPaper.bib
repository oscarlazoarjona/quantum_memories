title = {{Experimental demonstration of quantum memory for light}},
url = {http://www.nature.com/nature/journal/v432/n7016/full/nature03064.html$\backslash$nhttp://www.nature.com/doifinder/10.1038/nature03064},
volume = {432},
year = {2004}
