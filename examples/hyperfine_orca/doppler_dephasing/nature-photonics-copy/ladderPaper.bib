@article{Sibalic2016,
abstract = {We present, experimentally and theoretically, a scheme for dressed-state electromagnetically induced transparency (EIT) in a three-step cascade system where a four-level system is mapped into an effective three-level system. Theoretical analysis reveals that the scheme provides coherent state control via adiabatic following and provides a generalized protocol for light storage in uniform phase spin-waves that are insensitive to motional dephasing. The three-step driving enables a number of other features including spatial selectivity of the excitation region within the atomic medium, and kick-free and Doppler-free excitation that produces narrow resonances in thermal vapor. As a proof of concept we present an experimental demonstration of the generalized EIT scheme using the \$6S\_\{1/2\} \backslash rightarrow 6P\_\{3/2\} \backslash rightarrow 7S\_\{1/2\} \backslash rightarrow 8P\_\{1/2\}\$ excitation path in thermal cesium vapor. This technique could be applied to cold and thermal ensembles to enable longer storage times for Rydberg polaritons.},
archivePrefix = {arXiv},
arxivId = {1607.08031},
author = {\v{S}ibali\'{c}, N. and Kondo, J. M. and Adams, C. S. and Weatherill, K. J.},
doi = {10.1103/PhysRevA.94.033840},
eprint = {1607.08031},
file = {:C$\backslash$:/Users/kaczmarek/Downloads/PhysRevA.94.033840.pdf:pdf},
issn = {24699934},
journal = {Physical Review A},
mendeley-groups = {LadderPaper},
number = {3},
title = {{Dressed-state electromagnetically induced transparency for light storage in uniform-phase spin waves}},
volume = {94},
year = {2016}
}


@article{Ripka2016,
abstract = {We present a pulsed four-wave mixing (FWM) scheme via a Rydberg state to create, store and retrieve collective Rydberg polaritons. The storage medium consists of a gas of thermal Rb atoms confined in a 220 \{$\backslash$mu\}m thick cell, which are heated above room temperature. The experimental sequence consists of a pulsed excitation of Rydberg polaritons via the D1 line, a variable delay or storage time, and a final retrieval pulse via the D2 line. The lifetime of the Rydberg polaritons is around 1.2 ns, almost entirely limited by the excitation bandwidth and the corresponding motional dephasing of the atoms. The presented scheme combined with a tightly confined atomic ensemble is a good candidate for a deterministic single-photon source, as soon as strong interactions in terms of a Rydberg blockade are added.},
archivePrefix = {arXiv},
arxivId = {1603.05924},
